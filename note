k8s:

cat <<EOF >./kustomization.yaml
apiVersion: batch/v1
kind: Job
metadata:
  name: run-netty
spec:
  template:
    # This is the pod template
    spec:
      containers:
      - name: netty-server
        image: duongvt16/netty-app:test-52
      restartPolicy: OnFailure
EOF
+ get shell: kubectl exec --stdin --tty (pod name) -- /bin/bash
+ dynamuic yaml: 
        cat "kustomization.yaml" | sed "s/{{BUILD_ID}}/$BUILD_ID/g" | kubectl apply -f -
